(func (+ x y) (x.+ y))
(func (- x y) (x.- y))
(func (* x y) (x.* y))
(func (/ x y) (x./ y))
(func (% x y) (x.% y))
(func (= x y) (x.= y))
(func (!= x y) (x.!= y))
(func (< x y) (x.< y))
(func (<= x y) (x.<= y))
(func (> x y) (x.> y))
(func (>= x y) (x.>= y))
(func (& a b) (a.& b))
(func (| a b) (a.| b))
(func (^ a b) (a.^ b))

(func (not x) (if x false true))

(func (print o) (puts (o.display)))
(func (println o) {
  (puts (o.display))
  (puts "\n")
})

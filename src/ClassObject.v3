class ClassObject(superclass: ClassObject, name: string, classFields: Array<Field>) extends Object {
	new() super(ClassObjects.classClass) {}

	def typeof()  -> Type   { return Type.Class; }
	def display() -> string { return Strings.format1("<class '%s'>", name); }
	def isTrue()  -> bool   { return true; }
}

component ClassObjects {
	def classClass = ClassObject.new(Objects.classBase, "class", []);
}


(set status "flip")
(func (flip-flop)
  (cond
    ((= status "flip") {
      (puts "i")
      (set-global status "flop")
    })
    ((= status "flop") {
      (puts "o")
      (set-global status "flip")
    })))
(func (flip-flop-n n) {
  (set i 0)
  (while (< i n) {
    (flip-flop)
    (set i (i.succ))
  })
  (puts "\n")
  n
})
--
=> "flip"
=> <func 'flip-flop'>
=> <func 'flip-flop-n'>
==
(flip-flop)
--
i=> "flop"
--
(flip-flop-n 0)
--

=> 0
--
(flip-flop-n 1)
--
i
=> 1
--
(flip-flop-n 2)
--
io
=> 2
--
(flip-flop-n 3)
--
ioi
=> 3
--
(flip-flop-n 30)
--
ioioioioioioioioioioioioioioio
=> 30
--
(flip-flop-n 4000)
--

=> 4000
--

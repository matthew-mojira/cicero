(class pair
  (field head ())
  (field tail ())
  (method (display)
    (+ "(" (+ ((get-field display self.head)) (+ " " (+ ((get-field display self.tail)) ")"))))
  )
)
(func (cons x y) {
  (set p (new pair))
  (set-field head p x)
  (set-field tail p y)
  p
})
; incomplete test suite
--
=> <class 'pair'>
=> <func 'cons'>
==
(println (cons 1 2))
--
(1 2)
=> ()
--
(println (cons 1 ()))
--
(1 ())
=> ()
--

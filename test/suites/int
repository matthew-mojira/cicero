(3.= 3.0) ; `int` != `double`
--
=> false
--
(= 0b10111 0x17)
--
=> true
--
(= 3 "hello")
--
=> false
--
(10.as double)
--
=> 10.0
--
(/ 22 7)
--
=> 3
--
(!= 356 356.0)
--
=> true
--
(!= 356 358)
--
=> true
--
(<= -147_181 -10000000010)
--
=> false
--
(< -147_181 10)
--
=> true
--
(> -147_181 -2_384_288)
--
=> true
--
(> 181 119)
--
=> true
--
(>= 7_181 7_181)
--
=> true
--
(>= 7_179 7_180)
--
=> false
--
(<= -181 -181)
--
=> true
--
(<= -181 -182)
--
=> false
--
(try { (set x (* BIG_INT 1000)) (x.try double) } else "pass")
--
=> "pass"
--
(try { (set x (* BIG_INT 1000)) (x.as double) } else "fail")
--
=> Infinity
--
(try { (set x (* BIG_INT -1000)) (x.as double) } else "fail")
--
=> -Infinity
--
(try { (set x (* BIG_INT -1000)) (x.try double) } else "pass")
--
=> "pass"
